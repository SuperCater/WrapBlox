{
	"compilerOptions": {
	  "target": "ESNext",
	  "module": "NodeNext",
	  "moduleResolution": "Node16",
	  "lib": ["ES2020", "DOM"],
	  "rootDir": "./src",
	  "outDir": "dist",
	  "strict": true,
	  "strictPropertyInitialization": false,
	  "resolveJsonModule": true,
	  "sourceMap": true,
	  "allowJs": true,
	  "allowSyntheticDefaultImports": true,
	  "esModuleInterop": true,
	  "declaration": true,
	},
	"include": ["src"],
  }